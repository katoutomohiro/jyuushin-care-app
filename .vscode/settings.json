{
  // ========== コンソールエラー完全撲滅設定 ==========
  
  // WebSocketエラーを完全無効化
  "remote.autoForwardPorts": false,
  "remote.restoreForwardedPorts": false,
  "remote.tunnels.access": "none",
  "workbench.experimental.cloudChanges": false,
  "workbench.experimental.settingsProfiles": false,
  "dev.tunnels.logging": false,
  "dev.tunnels.access": "none",
  "remote.downloadExtensionsLocally": false,
  "remote.SSH.showLoginTerminal": false,
  "remote.SSH.remoteServerListenOnSocket": false,
  "remote.tunnels.alwaysUpdateCLI": false,
  
  // 拡張機能マーケットプレースの完全無効化
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "workbench.extensionManagement.enabled": false,
  "workbench.extensions.installVSIXConfirmation": false,
  
  // Content Security Policy関連エラーを無効化
  "security.allowedUNCHosts": [],
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.emptyWindow": false,
  
  // 外部通信を全て遮断
  "telemetry.telemetryLevel": "off",
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "typescript.surveys.enabled": false,
  "typescript.suggest.autoImports": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "git.autofetch": false,
  "git.autoStash": false,
  "npm.fetchOnlinePackageInfo": false,
  
  // Extension Host不安定性を解決
  "extensions.experimental.affinity": {
    "github.copilot": 1,
    "github.copilot-chat": 1,
    "github.codespaces": 1
  },
  "workbench.experimental.extensionHostKind": "localProcess",
  "remote.extensionKind": {},
  "remote.extensionHostRestart.enabled": false,
  "remote.extensionHostRestart.frequency": 0,
  "remote.portsAttributes": {},
  
  // 外部通信とサービスを無効化
  "scm.inputFontFamily": "default",
  "scm.repositories.visible": 0,
  
  // Service Worker関連エラーを無効化
  "workbench.webview.experimental.useIframes": false,
  "workbench.webview.experimental.additionalUrlQueryParameters": {},
  "workbench.experimental.webAPI": false,
  "workbench.experimental.useFileSystemProviders": false,
  "workbench.webview.experimental.disableServiceWorker": true,
  "workbench.webview.experimental.localResourceRoots": [],
  "workbench.localHistory.enabled": false,
  "workbench.cloudChanges.enabled": false,
  "workbench.experimental.enableNewProfilesUI": false,
  
  // User-Agent関連エラーを無効化
  "http.proxy": "",
  "http.proxySupport": "off",
  "http.proxyAuthorization": null,
  "http.proxyStrictSSL": false,
  
  // 不要なUI要素を完全無効化
  "editor.minimap.enabled": false,
  "editor.codeLens": false,
  "editor.lightbulb.enabled": "off",
  "breadcrumbs.enabled": false,
  "workbench.statusBar.visible": false,
  "workbench.sideBar.location": "left",
  "workbench.panel.defaultLocation": "bottom",
  "workbench.tips.enabled": false,
  "workbench.welcome.enabled": false,
  "workbench.startupEditor": "none",
  
  // ターミナルとタスクの最適化
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.experimentalUseTitleEvent": false,
  "terminal.integrated.experimentalLinkProvider": false,
  "task.autoDetect": "off",
  "task.showDecorations": false,
  
  // デバッグ関連の最適化
  "debug.console.acceptSuggestionOnEnter": "off",
  "debug.inlineValues": "off",
  "debug.showBreakpointsInOverviewRuler": false,
  "debug.showInStatusBar": "never",
  
  // 検索とファイルの最適化
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/.env*": true,
    "**/secrets*": true,
    "**/dist": true,
    "**/build": true,
    "**/.vscode": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/.env*": true,
    "**/secrets*": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
  },
  
  // エラーログの最小化
  "problems.showCurrentInStatus": false,
  "problems.decorations.enabled": false,
  "outline.showFiles": false,
  "outline.showModules": false,
  "outline.showPackages": false,
  "outline.showKeys": false,
  "outline.showNull": false,
  "outline.showBooleans": false,
  "outline.showArrays": false,
  "outline.showObjects": false,
  "outline.showNumbers": false,
  "outline.showStrings": false,
  
  // リスナーリーク防止
  "editor.suggest.showMethods": false,
  "editor.suggest.showFunctions": false,
  "editor.suggest.showConstructors": false,
  "editor.suggest.showFields": false,
  "editor.suggest.showVariables": false,
  "editor.suggest.showClasses": false,
  "editor.suggest.showStructs": false,
  "editor.suggest.showInterfaces": false,
  "editor.suggest.showModules": false,
  "editor.suggest.showProperties": false,
  "editor.suggest.showEvents": false,
  "editor.hover.enabled": false,
  "editor.parameterHints.enabled": false,
  
  // 完全エラー抑制
  "extensions.experimental.useUtilityProcess": false,
  "workbench.experimental.enableNewProfilesUI": false,
  "workbench.experimental.settingsProfile": false,
  "window.experimental.useSandbox": false,
  "window.experimental.windowControlsOverlay.enabled": false,
  "workbench.experimental.cloudChanges": false,
  "workbench.editor.experimental.editorTabs.enabled": false,
  "typescript.experimental.enableProjectDiagnostics": false,
  "javascript.experimental.enableProjectDiagnostics": false,
  "git.experimental.openDiffInEditor": false,
  "scm.experimental.showHistoryGraph": false,
  "debug.experimental.enableBreakpointsView": false,
  "terminal.experimental.enablePersistentSessions": false,
  "search.experimental.enableRegexMatchRanges": false,
  "git.ignoreLimitWarning": true,
  "workbench.activityBar.location": "hidden"
}
